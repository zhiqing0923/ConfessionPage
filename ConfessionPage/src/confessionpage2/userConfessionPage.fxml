<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="700.0" prefWidth="1080.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="confessionpage2.UserConfessionPageController">
   <children>
      <Label layoutX="466.0" layoutY="23.0" text="This is user confession page" />
      <TextField fx:id="confessionTextField" alignment="TOP_LEFT" layoutX="134.0" layoutY="212.0" prefHeight="286.0" prefWidth="811.0" promptText="Enter your confession here">
         <font>
            <Font size="18.0" />
         </font></TextField>
      <Button fx:id="submitButton" layoutX="500.0" layoutY="529.0" mnemonicParsing="false" onAction="#submitConfession" text="Submit">
         <font>
            <Font size="18.0" />
         </font></Button>
      <Button fx:id="backButton" layoutX="14.0" layoutY="12.0" mnemonicParsing="false" onAction="#confessionPage" text="&lt;Back">
         <font>
            <Font size="18.0" />
         </font></Button>
      <TextField fx:id="idTextField" layoutX="134.0" layoutY="150.0" prefHeight="40.0" prefWidth="259.0" promptText="Reply to ID(optional) : ">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
   </children>
</AnchorPane>
